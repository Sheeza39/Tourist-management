<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Visitors</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <header>
        <h1>Visitors</h1>
    </header>
    <main>
        <h2>Visitor List</h2>
        <ul>
            <% visitors.forEach(function(visitor) { %>
                <li>
                    <h3><%= visitor.name %></h3>
                    <p>Email: <%= visitor.email %></p>
                    <p>Reviewed Attractions: <%= visitor.reviewCount %></p>
                </li>
            <% }) %>
        </ul>
    </main>
    <footer>
        <p>Â© 2024 Tourist Management System</p>
    </footer>
</body>
</html>
